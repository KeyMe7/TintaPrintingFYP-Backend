# Server
PORT=3000
NODE_ENV=production
BACKEND_URL=https://tintaprintingfyp-backend.onrender.com
NODE_VERSION=25

# Firebase (Realtime Database)
FIREBASE_DATABASE_URL=https://tintaprintingfyp-default-rtdb.asia-southeast1.firebasedatabase.app
# Convert your serviceAccountKey.json to a single line (PowerShell example):
#   Get-Content serviceAccountKey.json -Raw | ConvertTo-Json -Compress
FIREBASE_SERVICE_ACCOUNT={"type":"service_account","project_id":"tintaprintingfyp","private_key_id":"REPLACE_PRIVATE_KEY_ID","private_key":"-----BEGIN PRIVATE KEY-----\nREPLACE_PRIVATE_KEY_BODY\n-----END PRIVATE KEY-----\n","client_email":"firebase-adminsdk@tintaprintingfyp.iam.gserviceaccount.com","client_id":"REPLACE_CLIENT_ID","auth_uri":"https://accounts.google.com/o/oauth2/auth","token_uri":"https://oauth2.googleapis.com/token","auth_provider_x509_cert_url":"https://www.googleapis.com/oauth2/v1/certs","client_x509_cert_url":"https://www.googleapis.com/robot/v1/metadata/x509/firebase-adminsdk%40tintaprintingfyp.iam.gserviceaccount.com"}

